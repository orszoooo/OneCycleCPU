#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 10;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55d6f29c8fe0 .scope module, "cpu_pc_tb" "cpu_pc_tb" 2 3;
 .timescale -9 -10;
v0x55d6f29fda60_0 .var "CLK", 0 0;
v0x55d6f29fdb50_0 .net "PC_OUT", 3 0, v0x55d6f29fd5b0_0;  1 drivers
v0x55d6f29fdc20_0 .var "RST", 0 0;
S_0x55d6f29c9160 .scope module, "UUT" "cpu_pc" 2 7, 3 3 0, S_0x55d6f29c8fe0;
 .timescale -9 -10;
    .port_info 0 /INPUT 1 "CLK"
    .port_info 1 /INPUT 1 "RST"
    .port_info 2 /OUTPUT 4 "PC_OUT"
v0x55d6f29c6130_0 .net "CLK", 0 0, v0x55d6f29fda60_0;  1 drivers
v0x55d6f29fd5b0_0 .var "PC_OUT", 3 0;
v0x55d6f29fd690_0 .net "RST", 0 0, v0x55d6f29fdc20_0;  1 drivers
E_0x55d6f29c4760 .event posedge, v0x55d6f29c6130_0;
S_0x55d6f29fd7b0 .scope task, "WAIT" "WAIT" 2 41, 2 41 0, S_0x55d6f29c8fe0;
 .timescale -9 -10;
v0x55d6f29fd980_0 .var "DY", 31 0;
TD_cpu_pc_tb.WAIT ;
    %load/vec4 v0x55d6f29fd980_0;
T_0.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/u;
    %jmp/1 T_0.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x55d6f29c4760;
    %jmp T_0.0;
T_0.1 ;
    %pop/vec4 1;
    %end;
    .scope S_0x55d6f29c9160;
T_1 ;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55d6f29fd5b0_0, 0;
    %end;
    .thread T_1;
    .scope S_0x55d6f29c9160;
T_2 ;
    %wait E_0x55d6f29c4760;
    %load/vec4 v0x55d6f29fd690_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55d6f29fd5b0_0, 0;
    %jmp T_2.1;
T_2.0 ;
    %load/vec4 v0x55d6f29fd5b0_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x55d6f29fd5b0_0, 0;
T_2.1 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55d6f29c8fe0;
T_3 ;
    %vpi_call 2 14 "$display", "Simulation of %m started." {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d6f29fdc20_0, 0, 1;
    %pushi/vec4 10, 0, 32;
    %store/vec4 v0x55d6f29fd980_0, 0, 32;
    %fork TD_cpu_pc_tb.WAIT, S_0x55d6f29fd7b0;
    %join;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55d6f29fdc20_0, 0, 1;
    %pushi/vec4 2, 0, 32;
    %store/vec4 v0x55d6f29fd980_0, 0, 32;
    %fork TD_cpu_pc_tb.WAIT, S_0x55d6f29fd7b0;
    %join;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d6f29fdc20_0, 0, 1;
    %pushi/vec4 20, 0, 32;
    %store/vec4 v0x55d6f29fd980_0, 0, 32;
    %fork TD_cpu_pc_tb.WAIT, S_0x55d6f29fd7b0;
    %join;
    %vpi_call 2 24 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x55d6f29c8fe0;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d6f29fda60_0, 0, 1;
T_4.0 ;
    %delay 50, 0;
    %load/vec4 v0x55d6f29fda60_0;
    %inv;
    %store/vec4 v0x55d6f29fda60_0, 0, 1;
    %jmp T_4.0;
    %end;
    .thread T_4;
    .scope S_0x55d6f29c8fe0;
T_5 ;
    %vpi_call 2 35 "$dumpfile", "cpu_sim.vcd" {0 0 0};
    %vpi_call 2 36 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55d6f29c9160 {0 0 0};
    %vpi_call 2 37 "$dumpon" {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "cpu_pc_tb.v";
    "cpu_pc.v";
